{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "Seq": {
    "ServerUrl": "http://localhost:5341",
    "ApiKey": "1234567890",
    "LogLevel": {
      "System": "Information",
      "Microsoft": "Warning"
    }
  },
  "MSSQL": {
    "Server": "localhost",
    "Database": "dev",
    "User": "sa",
    "Password": "Pass@word",
    "TrustServerCertificate": "true"
  },
  "Redis": {
    "Server": "127.0.0.1:6379"
  },
  "Serilog": {
    "Using": [],
    "MinimumLevel": {
      "Default": "Verbose",
      "Override": {
        "Microsoft": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "restrictedToMinimumLevel": "Debug"
        }
      }
    ],
    "Enrich": [ "FromLogContext" ],
    "Properties": {
      "Application": "DEMO-API"
    }
  },
  "AllowedHosts": "*"
}
